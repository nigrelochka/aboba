x=0
s=u"право"
z=u"лево"
def setup():
    size(600,600)
    strokeWeight(10)
    point(x+random(100,600),random(100,500))
def draw():  
    textAlign(CENTER,CENTER)
    textSize(40)
    fill(0)  
    text(koordinati(x),300,300)
def koordinati(x):
    if x>300:
        return s  
    else:
        return z   
   
